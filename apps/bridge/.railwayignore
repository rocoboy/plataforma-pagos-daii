docker-compose*.yml
.dockerignore

# Tests
test-sender.js
*.test.ts
*.spec.ts
__tests__/

# Node modules (se instalan en el build)
node_modules/

# Environment files (usar Railway variables)
.env
.env.*
env.example

# Git
.git/
.gitignore

# IDE
.vscode/
.idea/
*.swp
*.swo

# OS
.DS_Store
