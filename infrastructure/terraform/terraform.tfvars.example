# Copia este archivo a terraform.tfvars y configura tus valores

# ID del team de Vercel (puedes encontrarlo en el dashboard de Vercel)
vercel_team_id = "team_xxxxxxxxxxxxxxxx"

# Nombre del proyecto (opcional, usa el default si no especificas)
project_name = "plataforma-pagos-daii"

# Entorno (production, preview, development)
environment = "production"

# Rama de Git
branch = "main"

